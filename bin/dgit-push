#!/bin/env bash

#
# push code to remote
#
Syntax()
{
  echo "Syntax:
  [<option>]* <verb> [<parameters>]*
Where:
  <options may be one of:>
  -h|--help: this help
  " >&2
  exit 4
}

if [ $# -eq 0 ]; then
  Syntax
fi

if [ "${1}x" = '-hx' ] || [ "${1}x" = '--helpx' ]; then
  Syntax
fi
if [[ ${1:0:1} == "-" ]] ; then 
  echo "Unknown option: $1" >&2
  Syntax
fi

echo "dgit-push $@"
